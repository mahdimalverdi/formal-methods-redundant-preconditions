{
  "title": "Validate that `level` is within [0, 100]",
  "story": "Set a boolean-like flag for whether an input value falls in the acceptable range.",
  "category": "validation/range",
  "inputs": {
    "level": {
      "min": -10,
      "max": 300
    }
  },
  "step_limit": 100,
  "pre": [
    "level >= -10",
    "level <= 300",
    "level >= -1000",
    "level <= 1100"
  ],
  "post": [
    "(valid == 1 and level >= 0 and level <= 100) or (valid == 0 and (level < 0 or level > 100))"
  ],
  "program": [
    {
      "if": {
        "cond": "level >= 0 and level <= 100",
        "then": [
          {
            "assign": {
              "valid": "1"
            }
          }
        ],
        "else": [
          {
            "assign": {
              "valid": "0"
            }
          }
        ]
      }
    }
  ],
  "id": "ex_046",
  "expected_redundant_pre": [
    "level >= -1000",
    "level <= 1100"
  ]
}