{
  "title": "Order a pair of bounds (swap if needed)",
  "story": "Users may enter a range in any order. Normalize it so the output always satisfies lo <= hi.",
  "category": "branch/swap",
  "inputs": {
    "start": {
      "min": -50,
      "max": 50
    },
    "end": {
      "min": -50,
      "max": 50
    }
  },
  "step_limit": 100,
  "pre": [
    "start >= -50",
    "end >= -50",
    "start >= -150",
    "end >= -150"
  ],
  "post": [
    "lo <= hi and ((start <= end and lo == start and hi == end) or (start > end and lo == end and hi == start))"
  ],
  "program": [
    {
      "if": {
        "cond": "start <= end",
        "then": [
          {
            "assign": {
              "lo": "start",
              "hi": "end"
            }
          }
        ],
        "else": [
          {
            "assign": {
              "lo": "end",
              "hi": "start"
            }
          }
        ]
      }
    }
  ],
  "id": "ex_052",
  "expected_redundant_pre": [
    "start >= -150",
    "end >= -150"
  ]
}