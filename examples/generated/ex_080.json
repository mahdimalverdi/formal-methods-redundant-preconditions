{
  "title": "Minimum of two values",
  "story": "Pick the smaller of two candidate values (e.g., two shipping offers) and store it as the chosen minimum.",
  "category": "branch/min",
  "inputs": {
    "left": {
      "min": -10,
      "max": 10
    },
    "right": {
      "min": -10,
      "max": 10
    }
  },
  "step_limit": 80,
  "pre": [
    "left >= -10",
    "right >= -10",
    "left >= -20",
    "right >= -20"
  ],
  "post": [
    "min_val <= left and min_val <= right and ((left <= right and min_val == left) or (left > right and min_val == right))"
  ],
  "program": [
    {
      "if": {
        "cond": "left <= right",
        "then": [
          {
            "assign": {
              "min_val": "left"
            }
          }
        ],
        "else": [
          {
            "assign": {
              "min_val": "right"
            }
          }
        ]
      }
    }
  ],
  "id": "ex_080",
  "expected_redundant_pre": [
    "left >= -20",
    "right >= -20"
  ]
}