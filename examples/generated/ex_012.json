{
  "title": "Validate that `level` is within [0, 120]",
  "story": "Set a boolean-like flag for whether an input value falls in the acceptable range.",
  "category": "validation/range",
  "inputs": {
    "level": {
      "min": -999,
      "max": 999
    }
  },
  "step_limit": 100,
  "pre": [
    "level >= -999",
    "level <= 999",
    "level >= -1000",
    "level <= 1120"
  ],
  "post": [
    "(valid == 1 and level >= 0 and level <= 120) or (valid == 0 and (level < 0 or level > 120))"
  ],
  "program": [
    {
      "if": {
        "cond": "level >= 0 and level <= 120",
        "then": [
          {
            "assign": {
              "valid": "1"
            }
          }
        ],
        "else": [
          {
            "assign": {
              "valid": "0"
            }
          }
        ]
      }
    }
  ],
  "id": "ex_012",
  "expected_redundant_pre": [
    "level >= -1000",
    "level <= 1120"
  ]
}