{
  "title": "Validate that `score` is within [0, 120]",
  "story": "Set a boolean-like flag for whether an input value falls in the acceptable range.",
  "category": "validation/range",
  "inputs": {
    "score": {
      "min": -10,
      "max": 300
    }
  },
  "step_limit": 100,
  "pre": [
    "score >= -10",
    "score <= 300",
    "score >= -1000",
    "score <= 1120"
  ],
  "post": [
    "(valid == 1 and score >= 0 and score <= 120) or (valid == 0 and (score < 0 or score > 120))"
  ],
  "program": [
    {
      "if": {
        "cond": "score >= 0 and score <= 120",
        "then": [
          {
            "assign": {
              "valid": "1"
            }
          }
        ],
        "else": [
          {
            "assign": {
              "valid": "0"
            }
          }
        ]
      }
    }
  ],
  "id": "ex_004",
  "expected_redundant_pre": [
    "score >= -1000",
    "score <= 1120"
  ]
}