{
  "title": "Order a pair of bounds (swap if needed)",
  "story": "Users may enter a range in any order. Normalize it so the output always satisfies lo <= hi.",
  "category": "branch/swap",
  "inputs": {
    "start": {
      "min": -50,
      "max": 50
    },
    "end": {
      "min": -50,
      "max": 50
    }
  },
  "step_limit": 100,
  "pre": [
    "start >= -50",
    "end >= -50",
    "start >= -150",
    "end >= -150"
  ],
  "post": [
    "begin <= finish and ((start <= end and begin == start and finish == end) or (start > end and begin == end and finish == start))"
  ],
  "program": [
    {
      "if": {
        "cond": "start <= end",
        "then": [
          {
            "assign": {
              "begin": "start",
              "finish": "end"
            }
          }
        ],
        "else": [
          {
            "assign": {
              "begin": "end",
              "finish": "start"
            }
          }
        ]
      }
    }
  ],
  "id": "ex_017",
  "expected_redundant_pre": [
    "start >= -150",
    "end >= -150"
  ]
}